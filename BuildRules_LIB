	  m4_ifelse(_RUNTIME_,'static',`-MTd',`-MDd')) \
	m4_ifelse("_BUILD_RELEASE_","release",
    `-EHs-c- -GR- -Ox -Os -Oi -Ob2 -Oy- -Gy -GF',
		`-D_DEBUG -Od -Zi') \


